<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Student_Operation"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Student_Operation">

    <Button
        android:id="@+id/Student_View_past"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Past Course"
        app:layout_constraintBottom_toTopOf="@+id/Student_View_future"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <Button
        android:id="@+id/Student_View_future"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Desired Course"
        app:layout_constraintBottom_toTopOf="@+id/Student_Generate_Timeline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <Button
        android:id="@+id/Student_Generate_Timeline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generate Timeline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.60" />

    <Button
        android:id="@+id/Go_Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>