<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.myapplication.AdminLogin"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.myapplication.AdminCourseManagement"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_AdminCreate"
            app:destination="@id/AdminCreate" />
    </fragment>
    <fragment
        android:id="@+id/AdminCreate"
        android:name="com.example.myapplication.AdminCreateCourse"
        android:label="admin_add_new_course"
        tools:layout="@layout/admin_add_new_course">

        <action
            android:id="@+id/action_AdminCreate_to_SecondFragment"
            app:destination="@id/AdminCreate"/>
    </fragment>
</navigation>